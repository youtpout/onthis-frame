{
  "name": "framesjs-starter",
  "version": "0.2.6",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/dev-script.js",
    "dev:starter": "concurrently --kill-others \"next dev\" \"frames\"",
    "dev:monorepo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "azure": "./node_modules/next/dist/bin/next start",
    "update-debugger": "npx degit github:framesjs/frames.js/examples/framesjs-starter/app/debug#main app/debug --force",
    "update-framesjs": "yarn upgrade frames.js@latest && yarn run update-debugger"
  },
  "dependencies": {
    "@farcaster/core": "^0.14.3",
    "@xmtp/frames-validator": "^0.5.0",
    "autoprefixer": "^10.4.19",
    "frames.js": "^0.9.3",
    "next": "^14.1.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "@types/react": "^18.2.0",
    "@types/node": "^18.17.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@frames.js/debugger": "^0.1.9",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}